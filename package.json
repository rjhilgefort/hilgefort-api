  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "yarn clean && yarn install",
    "start": "node src/index.js",
    "start:watch": "onchange -i -k '**/*.js' '**/*.graphql' '.env' -- yarn start",
