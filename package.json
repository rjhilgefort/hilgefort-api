{
  "name": "rjhilgefort-monorepo",
  "author": "Rob Hilgefort <rjhilgefort@gmail.com>",
  "license": "MIT",
  "scripts": {
    "install:all": "yarn install && lerna exec --parallel -- yarn install",
    "clean": "rm -rf node_modules; lerna exec --parallel -- rm -rf node_modules",
    "reinstall": "yarn clean && yarn install:all",
    "start": "lerna run start --stream"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/node": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "lerna": "^3.10.7",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "prettier": "^1.11.1",
    "prisma": "^1.25.1",
    "treis": "^2.6.0"
  }
}
