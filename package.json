    "prisma:deploy": "prisma deploy",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dedent": "^0.7.0",
    "dotenv": "^6.2.0",
    "googleapis": "^36.0.0",
    "graphql-yoga": "^1.17.0",
    "hm-def": "^1.0.2",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "prisma-client-lib": "^1.25.0",
    "ramda": "^0.26.1",
    "sanctuary": "^0.15.0",
    "sanctuary-def": "^0.18.1",
    "sanctuary-type-classes": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/node": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "prisma": "^1.25.1",
    "treis": "^2.6.0"
  }
